    "postcss-import": "^12.0.1",
    "preact-cli": "^2.0.0"
  },
  "dependencies": {
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "randomcolor": "^0.5.3",
    "strftime": "^0.10.0"
