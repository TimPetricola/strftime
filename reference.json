    {
      "format": "y",
      "label": "The year without the century (00-99)"
    },
    {
      "format": "Y",
      "label": "The year with the century"
    },
    {
      "format": "z",
      "label": "The <code>+hhmm</code> or <code>-hhmm</code> numeric timezone (that is, the hour and minute offset from UTC)"
