    {
      "flag": "-",
      "label": "(dash) Omit leading zeroes or spaces (.e.g <code>%-d</code>)"
    },
    {
      "flag": "0",
      "label": "Force the use of zeroes as padding characters (.e.g <code>%0e</code>)"
    }
  ]
