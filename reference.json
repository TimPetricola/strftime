    {
      "format": "a",
      "label": "Abbreviated weekday name"
    },
    {
      "format": "A",
      "label": "Full weekday name"
    },
    {
      "format": "b",
      "label": "Abbreviated month name"
    },
    {
      "format": "B",
      "label": "Full month name"
    },
    {
